<Window x:Class="AutobuskaStanica.Forme.FrmParking"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutobuskaStanica.Forme"
        mc:Ignorable="d"
        Title="FrmParking" Height="450" Width="318">
    <Grid>
        <Button x:Name="btnSacuvaj" Content="Sačuvaj" HorizontalAlignment="Left" Margin="28,331,0,0" VerticalAlignment="Top" Width="75" Click="btnSacuvaj_Click"/>
        <Button x:Name="btnOtkazi" Content="Otkaži" HorizontalAlignment="Left" Margin="203,331,0,0" VerticalAlignment="Top" Width="75" Click="btnOtkazi_Click"/>
        <Label Content="Lokacija:" HorizontalAlignment="Left" Margin="47,66,0,0" VerticalAlignment="Top"/>
        <Label Content="Broj parkinga:" HorizontalAlignment="Left" Margin="47,140,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtLokacija" HorizontalAlignment="Left" Margin="135,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtBrParkinga" HorizontalAlignment="Left" Margin="135,144,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
